    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.0",
    "axios": "^1.3.4",
    "bech32": "^2.0.0",
    "lucid-cardano": "^0.9.8",
    "next": "13.2.4",
    "next-themes": "^0.2.1",
    "nextra": "^2.2.12",
    "nextra-theme-docs": "^2.2.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7"
